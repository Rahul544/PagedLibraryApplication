<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.pagedlibraryapplication" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".view.BottomTabActivity"
            android:label="@string/title_activity_bottom_tab" >
        </activity>

        <receiver
            android:name=".view.activity.MyReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".view.activity.SkwibbleTabActivity"
            android:label="@string/title_activity_skwibble_tab" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.LoginActivity" />
        <activity android:name=".view.activity.MainActivity" />
    </application>

</manifest>